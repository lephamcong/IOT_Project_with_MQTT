rule_engine {
  ignore_sys_message = true
  jq_function_default_timeout = 10s
  rules {
    rule_n8cs {
      actions = [
        {function = console}
      ]
      description = ""
      metadata {created_at = 1698719082653}
      sql = "SELECT\n  *,\n  mongo_date(timestamp) as timestamp,\n  json_encode(payload) as payload\nFROM\n  \"t/#\""
    }
  }
