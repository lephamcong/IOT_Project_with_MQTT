sysmon {
  os {
    cpu_check_interval = 60s
    cpu_high_watermark = 80%
    cpu_low_watermark = 60%
    procmem_high_watermark = 5%
    sysmem_high_watermark = 70%
  }
  vm {
    busy_dist_port = true
    busy_port = true
    large_heap = 32MB
    long_gc = disabled
    long_schedule = 240ms
    process_check_interval = 30s
    process_high_watermark = 80%
    process_low_watermark = 60%
  }
}
