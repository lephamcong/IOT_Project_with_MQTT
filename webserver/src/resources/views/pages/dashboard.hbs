<!-- Include scripts -->
<script>
    var datass = {{{ json datas }}};
    const dates = datass.map(item => item.date);

    var datas = datass[0];

    datas.timestamp = datas.timestamp.map((timestamp, index) => {
        if (index % 2 === 0) {
            return timestamp;
        } else {
            return "";
        }
    });
</script>
<script src="/js/dashboard.js"></script>
